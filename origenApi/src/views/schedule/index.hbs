<form action="" method="get">

</form>
<div class="row">
    {{#each schedule}}
    <div class="col-md-4">
        <div class="card">
            <div class="card-body">
                <a href="/schedule/edit/{{_id}}">
                    <i class="fas fa-pen-alt offset-md-10"></i>
                </a>
                <div class="col-md-6">
                    <h6>Hora Inicio</h6>
                    <label>{{timeInit}}</label>
                </div>
                <div class="col-md-6">
                    <h6>Hora Fin</h6>
                    <label>{{timeEnd}}</label>
                </div>
                <div class="col-md-12">
                    <p>{{description}}</p>
                </div>
                <div class="btn btn-danger btn-block btn-sm" onClick="window.location.href='/schedule/delete/{{_id}}'">
                    eliminar
                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>